foo 0 0 0 0 = 1
foo 1 0 0 0 = 2
foo 0 1 0 0 = 3
foo 0 0 1 0 = 4
foo 0 0 0 1 = 5
foo a b c d = a + b + c + d

bar 0 0 0 0 0 = 100
bar 1 1 1 1 1 = 200
bar a b c d e = a * b * c * d * e

main = do
    r1 := foo(0, 0, 0, 0)
    r2 := foo(1, 0, 0, 0)
    r3 := foo(2, 3, 4, 5)
    r4 := bar(0, 0, 0, 0, 0)
    r5 := bar(1, 1, 1, 1, 1)
    r6 := bar(2, 2, 2, 2, 2)
    r6
end
