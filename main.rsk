fib 0 = 0
fib 1 = 1
fib n = fib(n - 1) + fib(n - 2)

factorial 0 = 1
factorial n = n * factorial(n - 1)

main = do
  print("Computing Fibonacci(10)...")
  fibResult := fib(10)
  print("Fibonacci(10) =")
  print(fibResult)
  
  print("Computing factorial(5)...")
  factResult := factorial(5)
  print("factorial(5) =")
  print(factResult)
  
  sum := fibResult + factResult
  print("Sum =")
  print(sum)
  
  sum
end
